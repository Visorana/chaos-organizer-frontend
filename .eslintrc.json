{
  "plugins": ["prettier"],
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "env": {
    "es2017": true,
    "browser": true,
    "jest/globals": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn"
  },
  "globals": {
    "global": true,
    "require": "readonly",
    "process": "readonly"
  },
  "overrides": [
    {
      "files": ["**/*.test.js"],
      "plugins": ["jest"],
      "extends": ["plugin:jest/recommended"],
      "rules": {
      }
    }
  ]
}
